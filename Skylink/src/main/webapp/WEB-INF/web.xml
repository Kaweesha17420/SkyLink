<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
         
    <!-- Servlet for login -->
  		
  		<servlet>
  			<servlet-name>login</servlet-name>
  			<servlet-class>com.customer.LoginServlet</servlet-class>
  		</servlet>
  		
  		<servlet-mapping>
  			<servlet-name>login</servlet-name>
  			<url-pattern>/login</url-pattern>
  		</servlet-mapping>
  		
  	<!-- Servlet for signUp -->
  	
  		<servlet>
  			<servlet-name>sign</servlet-name>
  			<servlet-class>com.customer.CustomerInsert</servlet-class>
  		</servlet>
  		
  		<servlet-mapping>
  			<servlet-name>sign</servlet-name>
  			<url-pattern>/signup</url-pattern>
  		</servlet-mapping>
  		
  		
  	<!-- update customer -->
  		
  		<servlet>
		    <servlet-name>updateCustomerServlet</servlet-name>
		    <servlet-class>com.customer.updateCustomerServlet</servlet-class>
		</servlet>
		<servlet-mapping>
		    <servlet-name>updateCustomerServlet</servlet-name>
		    <url-pattern>/update</url-pattern>
		</servlet-mapping>
		
		
	<!-- delete customer -->
		
		<servlet>
			<servlet-name>deletecustomerservlet</servlet-name>
			<servlet-class>com.customer.deletecustomerservlet</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>deletecustomerservlet</servlet-name>
			<url-pattern>/delete</url-pattern>
		</servlet-mapping>
  	
  	<!-- Search packages -->
  	
  		<servlet>
			<servlet-name>search</servlet-name>
			<servlet-class>com.pkg.searchServlet</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>search</servlet-name>
			<url-pattern>/go</url-pattern>
		</servlet-mapping>
  
  
  	<!-- create packages -->
  	
  		<servlet>
  			<servlet-name>createpackage</servlet-name>
  			<servlet-class>com.pkg.InsertPackageServlet</servlet-class>
  		</servlet>
  		
  		<servlet-mapping>
  			<servlet-name>createpackage</servlet-name>
  			<url-pattern>/create</url-pattern>
  		</servlet-mapping>
  
  	<!-- update package -->
  		
  		<servlet>
		    <servlet-name>updatePackage</servlet-name>
		    <servlet-class>com.pkg.UpdatePackagesServlet</servlet-class>
		</servlet>
		<servlet-mapping>
		    <servlet-name>updatePackage</servlet-name>
		    <url-pattern>/upPack</url-pattern>
		</servlet-mapping>
		
	<!-- delete package -->
		
		<servlet>
			<servlet-name>deletepackage</servlet-name>
			<servlet-class>com.pkg.deletePackageServlet</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>deletepackage</servlet-name>
			<url-pattern>/delPack</url-pattern>
		</servlet-mapping>
		
	<!-- Servlet insert chat -->
  	
  		<servlet>
  			<servlet-name>chat</servlet-name>
  			<servlet-class>com.chat.InsertChatServlet</servlet-class>
  		</servlet>
  		
  		<servlet-mapping>
  			<servlet-name>chat</servlet-name>
  			<url-pattern>/send</url-pattern>
  		</servlet-mapping>
		
	<!-- delete chat -->
		
		<servlet>
			<servlet-name>deleteChat</servlet-name>
			<servlet-class>com.chat.deleteChatServlet</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>deleteChat</servlet-name>
			<url-pattern>/deleteC</url-pattern>
		</servlet-mapping>	
		
	 <!-- update chat -->
  		
  		<servlet>
		    <servlet-name>updateChat</servlet-name>
		    <servlet-class>com.chat.updateChatServlet</servlet-class>
		</servlet>
		<servlet-mapping>
		    <servlet-name>updateChat</servlet-name>
		    <url-pattern>/updateU</url-pattern>
		</servlet-mapping>	
		
		
	<!-- create Service -->
  	
  		<servlet>
  			<servlet-name>createService</servlet-name>
  			<servlet-class>com.self.InsertServiceServlet</servlet-class>
  		</servlet>
  		
  		<servlet-mapping>
  			<servlet-name>createService</servlet-name>
  			<url-pattern>/createS</url-pattern>
  		</servlet-mapping>
		
	
		
	<!-- delete Service -->
		
		<servlet>
			<servlet-name>ServiceDelete</servlet-name>
			<servlet-class>com.self.deleteServiceServlet</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>ServiceDelete</servlet-name>
			<url-pattern>/deleteSelf</url-pattern>
		</servlet-mapping>
		
	<!-- update service -->
  		
  		<servlet>
		    <servlet-name>updateService</servlet-name>
		    <servlet-class>com.self.updateServiceServlet</servlet-class>
		</servlet>
		<servlet-mapping>
		    <servlet-name>updateService</servlet-name>
		    <url-pattern>/updateS</url-pattern>
		</servlet-mapping>		
		
		
    <!-- Servlet for Admin login -->
  		
  		<servlet>
  			<servlet-name>Adminlogin</servlet-name>
  			<servlet-class>com.admin.AdminloginServlet</servlet-class>
  		</servlet>
  		
  		<servlet-mapping>
  			<servlet-name>Adminlogin</servlet-name>
  			<url-pattern>/log</url-pattern>
  		</servlet-mapping>	
		

  
</web-app>
